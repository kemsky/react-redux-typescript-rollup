{
  "name": "react-redux-typescript-rollup",
  "version": "1.0.0",
  "description": "React & Redux & Typescript & Rollup",
  "repository": "N/A",
  "license": "MIT",
  "engineStrict": false,
  "engines": {
    "yarn": "1.5.1",
    "node": "9.1.0",
    "npm": "5.5.1"
  },
  "scripts": {
    "prebuild": "yarn run clean && yarn run tsc && yarn run lint",
    "build": "cross-env NODE_ENV=production rollup -c",
    "start": "cross-env NODE_ENV=development rollup -cw",
    "lint": "tslint --project tsconfig.json",
    "tsc": "tsc --p tsconfig.json -outDir ./temp",
    "clean": "shx rm -rf ./temp && shx rm -rf ./build"
  },
  "author": "http://itmint.ca",
  "devDependencies": {
    "@types/react": "16.3.14",
    "@types/react-dom": "16.0.5",
    "@types/react-redux": "6.0.0",
